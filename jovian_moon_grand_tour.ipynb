   "outputs": [],
   "source": [
    "# Search for Europa-aligned perijove while keeping apojove below Callisto\n",
    "if 'final_io_assist_orbit' in locals():\n",
    "    print(\"\\n\" + \"=\" * 60)\n",
    "    print(\"EUROPA PERIJOVE RESONANCE SEARCH\")\n",
    "    print(\"=\" * 60)\n",
    "\n",
    "    europa_radius = placeholder_europa_orbit.a\n",
    "    callisto_radius = placeholder_callisto_orbit.a\n",
    "\n",
    "    io_orb_at_flyby = placeholder_io_orbit.propagate(final_io_flyby_date)\n",
    "    search_orbit = final_io_assist_orbit\n",
    "    search_date = final_io_flyby_date\n",
    "\n",
    "    r_p_test_values = np.linspace(50, 1500, 18) * u.km\n",
    "    best_europa_dv = 1000 * u.km/u.s\n",
    "    best_europa_config = None\n",
    "\n",
    "    # Enforce apojove below Callisto to keep the assist geometry stable\n",
    "    apojove_cap = 0.98 * callisto_radius\n",
    "\n",
    "    europa_period_ref = placeholder_europa_orbit.propagate(search_date).period\n",
    "\n",
    "    for test_rp_alt in r_p_test_values:\n",
    "        for test_sign in [-1, 1]:\n",
    "            test_rp = Io.R + test_rp_alt\n",
    "            rel_vel = search_orbit.rv()[1] - io_orb_at_flyby.rv()[1]\n",
    "            rel_speed = np.linalg.norm(rel_vel)\n",
    "\n",
    "            rot_ang = 2 * np.arcsin(1 / ((test_rp * rel_speed**2 / Io.k) + 1 * u.one))\n",
    "            axis = np.array([0, 0, 1.0]) / np.linalg.norm([0, 0, 1.0])\n",
    "            rot = scipyRot.from_rotvec(test_sign * axis * rot_ang.value)\n",
    "            rel_vel_out = rot.apply(rel_vel.value) * u.km / u.s\n",
    "            post_assist_vel = rel_vel_out + io_orb_at_flyby.rv()[1]\n",
    "\n",
    "            test_orbit = Orbit.from_vectors(\n",
    "                Jupiter,\n",
    "                io_orb_at_flyby.r,\n",
    "                post_assist_vel,\n",
    "                search_date,\n",
    "                plane=Planes.EARTH_ECLIPTIC,\n",
    "            )\n",
    "\n",
    "            if test_orbit.ecc >= 1 or test_orbit.r_p < Jupiter.R:\n",
    "                continue\n",
    "\n",
    "            # Keep apojove below Callisto to prevent the wonky high-apojove leg\n",
    "            if test_orbit.r_a > apojove_cap:\n",
    "                continue\n",
    "\n",
    "            peri_err = abs(test_orbit.r_p - europa_radius)\n",
    "            if peri_err > 0.08 * europa_radius:\n",
    "                continue\n",
    "\n",
    "            # Check for simple resonance with Europa\n",
    "            period_ratio = test_orbit.period / europa_period_ref\n",
    "            found_resonance = False\n",
    "            resonance_tag = None\n",
    "            for n_body in range(1, 9):\n",
    "                for n_sc in range(1, 9):\n",
    "                    expected = n_body / n_sc\n",
    "                    ratio_error = abs(period_ratio.value - expected) / expected\n",
    "                    if ratio_error < 0.03:\n",
    "                        resonance_tag = (n_sc, n_body)\n",
    "                        found_resonance = True\n",
    "                        break\n",
    "                if found_resonance:\n",
    "                    break\n",
    "            if resonance_tag is None:\n",
    "                continue\n",
    "\n",
    "            try:\n",
    "                test_dv, test_transfer, test_date, _, _ = get_single_burn_elliptical_hohmann_test(\n",
    "                    Jupiter,\n",
    "                    Europa,\n",
    "                    test_orbit,\n",
    "                    5*u.day,\n",
    "                    35*u.day,\n",
    "                    tof_step=0.2*u.day,\n",
    "                )\n",
    "            except Exception:\n",
    "                continue\n",
    "\n",
    "            if test_dv < best_europa_dv:\n",
    "                best_europa_dv = test_dv\n",
