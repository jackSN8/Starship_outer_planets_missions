    "                    'rp_alt': test_rp_alt,\n",
    "                    'sign': test_sign,\n",
    "                    'resonance': resonance_tag,\n",
    "                    'test_orbit': test_orbit,\n",
    "                    'transfer_orbit': test_transfer,\n",
    "                    'transfer_date': test_date,\n",
